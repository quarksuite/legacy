{
  "compilerOptions": {
    "target": "esnext",
    "module": "esnext",
    "moduleResolution": "node",
    "lib": [
      "esnext"
    ],
    "strict": true,
    "esModuleInterop": true
  },
  "exclude": ["node_modules", "tests"],
  "typedocOptions": {
    "exclude": "**/*+(.spec).ts",
    "excludeNotExported": true,
    "mode": "modules",
    "name": "Quarksilver (Core)",
    "theme": "minimal"
  }
}
